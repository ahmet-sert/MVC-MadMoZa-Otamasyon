@model MVC_ModMaZa.Models.DbSınıflar.FaturaKalem
@{
    ViewBag.Title = "YeniKalem";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}



<br />
<h2>Yeni Kalem Sayfası</h2>
<br />

<form class="form-group" method="post">
    <h4>Açıklama</h4>
    @Html.TextBoxFor(x => x.Açiklama, new { @class = "form-control" })
    <br />
    <h4>Miktar </h4>
    @Html.TextBoxFor(x => x.Miktar, new { @class = "form-control" })
    <br />
    <h4>Birim Fiyat </h4>
    @Html.TextBoxFor(x => x.BirimFiyatı, new { @class = "form-control" })
    <br />
    <h4>Tutar</h4>
    @Html.TextBoxFor(x => x.Tutar, new { @class = "form-control" })
    <br />
    <h4>Tutar</h4>
    @Html.TextBoxFor(x => x.FaturaID, new { @class = "form-control" })
    <br />
    <button class=" btn btn-primary"> Kaydet   </button>
</form>